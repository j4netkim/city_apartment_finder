<!DOCTYPE html>
<html>
  <head>
    <title>CityApartmentFinder</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <div>
      <% if logged_in? %>
        <%= link_to owners_path %>
        <%= link_to log_out_path, method: "delete" %>
      <% end %>
    </div>
    
  <%= button_to("Google Log In", '/auth/google_oauth2') %>

    <%= yield %>
  </body>
</html>
